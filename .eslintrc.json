{
  "extends": ["@rocketseat/eslint-config/next"],
  "plugins": ["simple-import-sort", "react-hooks"],
    "rules": {
      "simple-import-sort/imports": "error",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn" 
    },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}